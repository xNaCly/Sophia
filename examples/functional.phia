;; vim: syntax=lisp
(let arr 1 2 3 4 5 6 7 8 9)

(println "incremented:" 
    (map (lambda [n] (+ n 1)) arr))

(println "squared:" 
    (map (lambda [n] (* n n)) arr))

(println "even numbers:" 
    (filter (lambda [n] (= (% n 2) 0)) arr))

(println "natural numbers"
    (map (lambda [c] (- cc 97)) "hello world"))
